{
  "name": "lnfs",
  "version": "0.2.0",
  "description": "Futsal API and web",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "astro build",
    "dev:api": "wrangler dev api/index.js",
    "dev": "astro dev",
    "preview": "astro preview",
    "publish:api": "wrangler publish api/index.js",
    "scrape": "node scraping/index.js",
    "start": "astro dev",
    "test:cov": "vitest run --coverage",
    "test": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@astrojs/tailwind": "3.0.1",
    "astro": "2.0.4",
    "cheerio": "1.0.0-rc.12",
    "hono": "2.7.2",
    "picocolors": "1.0.0",
    "tailwindcss": "3.2.6"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "0.28.4",
    "eslint-plugin-astro": "0.23.0",
    "prettier": "2.8.3",
    "prettier-plugin-astro": "0.8.0",
    "standard": "17.0.0",
    "vite": "^4.1.1",
    "vitest": "0.27.1",
    "wrangler": "2.7.1"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:astro/recommended"
    ],
    "rules": {
      "space-before-function-paren": "off"
    },
    "overrides": [
      {
        "files": [
          "*.astro"
        ],
        "parser": "astro-eslint-parser",
        "rules": {
          "astro/no-set-html-directive": "error"
        }
      }
    ]
  }
}
